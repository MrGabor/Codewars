def convert(st):
    if st == "": return 0
    number = ""
    a = 0
    convertDict = {}
    letterArray = [1, 0, 2, 3, 4, 5, 6, 7, 8, 9]
    for letter in st.upper():
        if letter not in convertDict:
            convertDict[letter] = letterArray[a]
            a += 1
        number += str(convertDict[letter])
    return int(number)


print(convert("KATA"))
print(convert("KaTA"))
print(convert("CodeWars"))
print(convert("abababaabbbcde"))
